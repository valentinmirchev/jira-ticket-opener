{
  "name": "JIRA Ticket Opener",
  "description": "Open tickets in JIRA made easy",
  "version": "0.1.0",
  "manifest_version": 3,
  "icons": {
    "16": "./icons/16x16.png",
    "32": "./icons/32x32.png",
    "48": "./icons/48x48.png",
    "128": "./icons/128x128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./icons/16x16.png",
      "32": "./icons/32x32.png",
      "48": "./icons/48x48.png",
      "128": "./icons/128x128.png"
    }
  },
  "options_page": "options.html",
  "permissions": ["contextMenus", "scripting", "storage", "activeTab"]
}
